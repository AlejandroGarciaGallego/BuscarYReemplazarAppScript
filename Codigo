//Función para guardar las hojas de cálculo en variables, obtener la última fila y columna de las tablas.
function ActualizarPlan() {
  var libro = SpreadsheetApp.getActiveSpreadsheet();
  var TMovil = libro.getSheetByName("Copia de TMovil_Cargos");
  var PlanesDisponibles = libro.getSheetByName("Planes disponibles");
  TMovilUltimaFila = TMovil.getLastRow();
  TMovilUltimaColumna = TMovil.getLastColumn();
  PlanesDisponiblesUltimaFila = PlanesDisponibles.getLastRow();
  PlanesDisponiblesUltimaColumna = PlanesDisponibles.getLastColumn();
  
//Método para recorrer y obtener los datos de cada tabla.

  var DatosOriginales = TMovil.getRange(1,1,TMovilUltimaFila,TMovilUltimaColumna).getValues();
  //console.log(DatosOriginales[0]);
  var DatosPlanesDisponibles = PlanesDisponibles.getRange(6,1,PlanesDisponiblesUltimaFila, PlanesDisponiblesUltimaColumna).getValues();
  for (var i = 0; i < DatosOriginales.length; i++) {

//Métodos para hacer la búsqueda según cada regla, copiar y pegar la info que se requiere.

//Sección 1: Analista      

      if(DatosOriginales[i][3]=="CLARO" && DatosOriginales[i][11]=="ANALISTA"  && DatosOriginales[i][12]=="PLAN COMPAÑIA"){
       var iSuma = i+1;
       //console.log(iSuma);
      var Pegar = TMovil.getRange(iSuma,9);
      Pegar.setValue(DatosPlanesDisponibles[0][10]);
      var PegarCantGB = TMovil.getRange(iSuma,6);
      PegarCantGB.setValue(DatosPlanesDisponibles[0][6]);

      } else if(DatosOriginales[i][3]=="CLARO" && DatosOriginales[i][11]=="ANALISTA"  && DatosOriginales[i][12]=="PLAN BENEFICIO 1"){
       var iSuma = i+1;
       //console.log(iSuma);
      var Pegar = TMovil.getRange(iSuma,9);
      Pegar.setValue(DatosPlanesDisponibles[0][18]);
      var PegarCantGB = TMovil.getRange(iSuma,6);
      PegarCantGB.setValue(DatosPlanesDisponibles[0][16]);

      }else if(DatosOriginales[i][3]=="CLARO" && DatosOriginales[i][11]=="ANALISTA"  && DatosOriginales[i][12]=="PLAN BENEFICIO 2"){
       var iSuma = i+1;
       //console.log(iSuma);
      var Pegar = TMovil.getRange(iSuma,9);
      Pegar.setValue(DatosPlanesDisponibles[0][33]);
      var PegarCantGB = TMovil.getRange(iSuma,6);
      PegarCantGB.setValue(DatosPlanesDisponibles[0][28]);

    } else if(DatosOriginales[i][3]=="MOVISTAR" && DatosOriginales[i][11]=="ANALISTA"  && DatosOriginales[i][12]=="PLAN COMPAÑIA"){
       var iSuma = i+1;
       //console.log(iSuma);
      var Pegar = TMovil.getRange(iSuma,9);
      Pegar.setValue(DatosPlanesDisponibles[0][10]);
      var PegarCantGB = TMovil.getRange(iSuma,6);
      PegarCantGB.setValue(DatosPlanesDisponibles[0][8]);

    } else if(DatosOriginales[i][3]=="MOVISTAR" && DatosOriginales[i][11]=="ANALISTA"  && DatosOriginales[i][12]=="PLAN BENEFICIO 1"){
       var iSuma = i+1;
       //console.log(iSuma);
      var Pegar = TMovil.getRange(iSuma,9);
      Pegar.setValue(DatosPlanesDisponibles[0][21]);
      var PegarCantGB = TMovil.getRange(iSuma,6);
      PegarCantGB.setValue(DatosPlanesDisponibles[0][19]);

