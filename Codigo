      Pegar.setValue(DatosPlanesDisponibles[1][18]);
      var PegarCantGB = TMovil.getRange(iSuma,6);
      PegarCantGB.setValue(DatosPlanesDisponibles[1][16]);

    } else if(DatosOriginales[i][3]=="CLARO" && DatosOriginales[i][11]=="LIDER- DIRECTOR"  && DatosOriginales[i][12]=="PLAN BENEFICIO 2"){
       var iSuma = i+1;
       //console.log(iSuma);
      var Pegar = TMovil.getRange(iSuma,9);
      Pegar.setValue(DatosPlanesDisponibles[1][33]);
      var PegarCantGB = TMovil.getRange(iSuma,6);
      PegarCantGB.setValue(DatosPlanesDisponibles[1][28]);

    } else if(DatosOriginales[i][3]=="MOVISTAR" && DatosOriginales[i][11]=="LIDER- DIRECTOR"  && DatosOriginales[i][12]=="PLAN COMPAÑIA"){
       var iSuma = i+1;
       //console.log(iSuma);
      var Pegar = TMovil.getRange(iSuma,9);
      Pegar.setValue(DatosPlanesDisponibles[1][10]);
      var PegarCantGB = TMovil.getRange(iSuma,6);
      PegarCantGB.setValue(DatosPlanesDisponibles[1][8]);

    } else if(DatosOriginales[i][3]=="MOVISTAR" && DatosOriginales[i][11]=="LIDER- DIRECTOR"  && DatosOriginales[i][12]=="PLAN BENEFICIO 1"){
       var iSuma = i+1;
       //console.log(iSuma);
      var Pegar = TMovil.getRange(iSuma,9);
      Pegar.setValue(DatosPlanesDisponibles[1][21]);
      var PegarCantGB = TMovil.getRange(iSuma,6);
      PegarCantGB.setValue(DatosPlanesDisponibles[1][19]);

    } else if(DatosOriginales[i][3]=="MOVISTAR" && DatosOriginales[i][11]=="LIDER- DIRECTOR"  && DatosOriginales[i][12]=="PLAN BENEFICIO 2"){
       var iSuma = i+1;
       //console.log(iSuma);
      var Pegar = TMovil.getRange(iSuma,9);
      Pegar.setValue(DatosPlanesDisponibles[1][33]);
      var PegarCantGB = TMovil.getRange(iSuma,6);
      PegarCantGB.setValue(DatosPlanesDisponibles[1][30]);
    }



    }



  
